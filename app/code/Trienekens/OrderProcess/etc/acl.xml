<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
	<acl>
		<resources>
			<resource id="Magento_Backend::admin">
				<resource id="Trienekens_OrderProcess::OrderProcess" title="Order Process" sortOrder="10">
					<resource id="Trienekens_OrderProcess::OrderProcess_export" title="Export Order Process" sortOrder="10"/>
					<resource id="Trienekens_OrderProcess::OrderProcess_view" title="View Order Process" sortOrder="40"/>
				</resource>
				<resource id="Magento_Backend::stores">
					<resource id="Magento_Backend::stores_settings">
						<resource id="Magento_Config::config">
							<resource id="Trienekens_OrderProcess::config_section" title="Trienekens Order Process Configuration"/>
						</resource>
					</resource>
				</resource>
			</resource>
		</resources>
	</acl>
</config>
